{
  "name": "@chatie/server",
  "version": "0.6.6",
  "engines": {
    "node": "12"
  },
  "description": "Web Service for Chatie",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "tap tests/**/*.spec.ts",
    "start": "ts-node bin/server.ts",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:md",
    "lint:md": "markdownlint README.md",
    "lint:es": "eslint --ignore-pattern node_modules/ --ignore-pattern fixtures/ \"{bin,examples,src,scripts,tests}/**/*.ts\"",
    "lint:ts": "tsc --noEmit"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chatie/server.git"
  },
  "keywords": [
    "chatie",
    "chatie.io",
    "wechaty",
    "wechat",
    "bot manager",
    "cloud",
    "management"
  ],
  "author": "Huan LI <zixia@zixia.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/chatie/server/issues"
  },
  "homepage": "https://github.com/chatie/server#readme",
  "dependencies": {
    "@chatie/io": "^0.8.3",
    "brolog": "^1",
    "dotenv": "^9.0.1",
    "express": "^4.17.1",
    "pkg-dir": "^5.0.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@chatie/eslint-config": "^0.12.1",
    "@chatie/git-scripts": "^0.6.2",
    "@chatie/tsconfig": "^0.14.1",
    "@types/blue-tape": "^0.1.33",
    "@types/express": "^4.17.7",
    "@types/express-serve-static-core": "^4.17.9",
    "@types/mime": "^2.0.3",
    "@types/node": "^15.0.0",
    "@types/serve-static": "^1.13.5",
    "@types/ws": "^7.2.6",
    "blue-tape": "^1.0.0",
    "tap": "^15.0.5",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0"
  },
  "git": {
    "scripts": {
      "pre-push": "npx git-scripts-pre-push"
    }
  }
}
